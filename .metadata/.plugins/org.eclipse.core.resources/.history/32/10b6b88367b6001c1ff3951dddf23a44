package com.project.cat.common;

import java.sql.Date;

public class Paging extends SearchDate{
	
	private static final long serialVersionUID = 6225148861734528357L;
	private int startRow;
	private int endRow;
	private String keyword;

	public Paging() {}

	public Paging(int startRow, int endRow) {
		super();
		this.startRow = startRow;
		this.endRow = endRow;
	}
	
	public Paging(int startRow, int endRow, String keyword) {
		super();
		this.startRow = startRow;
		this.endRow = endRow;
		this.keyword = keyword;
	}
	
	public Paging(int startRow, int endRow, Date begin, Date end) {
		super(begin, end);
		this.startRow = startRow;
		this.endRow = endRow;

	}

	public int getStartRow() {
		return startRow;
	}

	public void setStartRow(int startRow) {
		this.startRow = startRow;
	}

	public int getEndRow() {
		return endRow;
	}

	public void setEndRow(int endRow) {
		this.endRow = endRow;
	}

	public String getkeyword() {
		return keyword;
	}

	public void setkeyword(String keyword) {
		this.keyword = keyword;
	}



	public void setEnd(Date end) {
		this.end = end;
	}

	@Override
	public String toString() {
		return "Paging [startRow=" + startRow + ", endRow=" + endRow + ", keyword=" + keyword + ", start=" + start
				+ ", end=" + end + "]";
	}

	
	
}
